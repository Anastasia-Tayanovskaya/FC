    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.19.0",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "classlist-polyfill": "^1.0.3",
